{
    "name": "noderssbot",
    "description": "Another Telegram RSSBot in Node.js",
    "repository": "https://github.com/loguu/NodeRSSBot",
    "logo": "https://i.loli.net/2019/04/23/5cbeb7e41414c.png",
    "keywords": ["RSS"],
    "env": {
        "NODE_MODULES_CACHE": {
            "value": "false",
            "required": true
        },
        "PORT": {
            "value": "80",
            "required": false
        },
        "RSSBOT_TOKEN": {
            "required": true
        }
    }
}
